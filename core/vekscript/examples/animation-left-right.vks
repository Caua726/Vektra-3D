import cube.obj as cube-001

object{ 
    obj: cube-001
    position: [0, 0, 0]
    rotation: [0, 0, 0]
    scale: [1, 1, 1]
}

keyframe(0) {
    object: cube-001{
        position: [0, 0, 0]
        rotation: [0, 0, 0]
        scale: [1, 1, 1]    
    }
}

keyframe(5) {
    object: cube-001{
        position: [1, 0, 0]
        rotation: [0, 0, 0]
        scale: [1, 1, 1]
    }
}

interpolate(0->5) {
    type: bezier
}